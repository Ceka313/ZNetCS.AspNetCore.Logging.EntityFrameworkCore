{
  "version": "1.0.0-*",
  "copyright": "Marcin Smółka zNET Computer Solutions",
  "authors": [ "Marcin Smółka" ],
  "testRunner": "mstest",

  "buildOptions": {
    "warningsAsErrors": false,
    "additionalArguments": [ "/ruleset:../../CommonRuleSet.ruleset", "/additionalfile:../../stylecop.json" ]
  },

  "dependencies": {
    "ZNetCS.AspNetCore.Logging.EntityFrameworkCore": {
      "version": "*",
      "target": "project"
    },
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    },
    "dotnet-test-mstest": "1.1.2-preview",
    "Microsoft.AspNetCore.TestHost": "1.1.0",
    "Microsoft.Extensions.Logging": "1.1.0",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "Microsoft.Extensions.Logging.Debug": "1.1.0",
    "Microsoft.Extensions.Logging.Filter": "1.1.0",
    "Microsoft.EntityFrameworkCore": "1.1.0",
    "Microsoft.EntityFrameworkCore.InMemory": "1.1.0",
    "MSTest.TestFramework": "1.0.7-preview"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],

      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.0",
          "type": "platform"
        }

      }
    }
  }
}
